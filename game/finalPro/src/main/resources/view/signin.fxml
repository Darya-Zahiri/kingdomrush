<AnchorPane prefHeight="319.0" prefWidth="542.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.Signin">
   <children>
      <ImageView fx:id="imageView" fitHeight="394.0" fitWidth="541.0" layoutY="-2.0" opacity="0.64" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../Pictures/Saved%20Pictures/sign.jpg" />
         </image>
         <effect>
            <Blend />
         </effect>
      </ImageView>
      <TextField fx:id="username" layoutX="154.0" layoutY="77.0" prefHeight="26.0" prefWidth="216.0" promptText="username" />
      <PasswordField fx:id="password" layoutX="154.0" layoutY="135.0" prefHeight="26.0" prefWidth="216.0" promptText="password" />
      <ImageView fx:id="signin" fitHeight="98.0" fitWidth="80.0" layoutX="222.0" layoutY="170.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../Pictures/Saved%20Pictures/pngtree-wooden-panel-for-game-ui-in-cartoon-style-png-image_9560501.png" />
         </image>
      </ImageView>
      <ImageView fx:id="signup" fitHeight="104.0" fitWidth="80.0" layoutX="222.0" layoutY="223.0" pickOnBounds="true" preserveRatio="true">
         <image>
            <Image url="@../../../../../../Pictures/Saved%20Pictures/pngtree-wooden-panel-for-game-ui-in-cartoon-style-png-image_9560501.png" />
         </image>
      </ImageView>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="229.0" layoutY="252.0" onMouseClicked="#setSignUp" prefHeight="20.0" prefWidth="65.0" text="signup">
         <font>
            <Font name="Trebuchet MS Bold" size="18.0" />
         </font>
      </Label>
      <Label alignment="CENTER" contentDisplay="CENTER" layoutX="229.0" layoutY="199.0" prefHeight="20.0" prefWidth="65.0" text="signin">
         <font>
            <Font name="Trebuchet MS Bold" size="18.0" />
         </font>
      </Label>
      <Label fx:id="error" alignment="CENTER" layoutX="-1.0" layoutY="38.0" prefHeight="18.0" prefWidth="541.0" textFill="RED">
         <font>
            <Font name="System Bold" size="18.0" />
         </font>
      </Label>
      <Button fx:id="signU" layoutX="228.0" layoutY="249.0" mnemonicParsing="false" onAction="#setSignUp" opacity="0.0" prefHeight="26.0" prefWidth="66.0" />
      <Button fx:id="signI" layoutX="228.0" layoutY="197.0" mnemonicParsing="false" onAction="#setSignIn" opacity="0.0" prefHeight="26.0" prefWidth="66.0" />
   </children>
</AnchorPane>
